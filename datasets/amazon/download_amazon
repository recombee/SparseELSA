
mkdir amazon_fashion
wget -P amazon_fashion https://datarepo.eng.ucsd.edu/mcauley_group/data/amazon_v2/metaFiles2/meta_AMAZON_FASHION.json.gz
wget -P amazon_fashion https://datarepo.eng.ucsd.edu/mcauley_group/data/amazon_v2/categoryFilesSmall/AMAZON_FASHION.csv
gunzip amazon_fashion/meta_AMAZON_FASHION.json.gz

